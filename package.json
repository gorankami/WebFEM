{
  "name": "g-web-fem",
  "version": "1.2.0",
  "description": "Web viewer for FEM models",
  "main": "index.js",
  "scripts": {
    "jshint": "./node_modules/.bin/jshint src",
    "test": "echo \"Error: no test specified\" && exit 1",
    "move-source": "mkdir dist && cp src/index.html dist/index.html && cp -r data dist",
    "build": "rm -rf dist && npm run move-source && webpack",
    "start": "http-server dist -p 1337 & npm run build"
  },
