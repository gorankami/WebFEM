  "bugs": {
    "url": "https://github.com/gorankami/WebFEM/issues"
  },
  "jshintConfig": {
    "esversion": 6
  },
  "homepage": "https://github.com/gorankami/WebFEM#readme",
  "devDependencies": {
    "css-loader": "^0.28.4",
    "html-loader": "^0.4.5",
    "http-server": "^0.10.0",
    "run-sequence": "^1.2.2",
    "style-loader": "^0.18.2",
    "webpack": "^3.0.0"
  },
  "dependencies": {
    "angular": "^1.5.7",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "bootstrap": "^3.3.6",
    "gl-matrix": "^2.3.2",
    "gl-matrix-mat4": "^2.2.1-npm",
